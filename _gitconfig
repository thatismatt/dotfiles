[credential]
        helper = cache --timeout=500000
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[push]
        default = current
[alias]
        l = log --pretty=format:'%C(auto)%h %Cgreen%an%Creset %Cblue%ar%Creset %s %C(auto)%d' --graph
[init]
        defaultBranch = main
[merge]
	conflictStyle = diff3
